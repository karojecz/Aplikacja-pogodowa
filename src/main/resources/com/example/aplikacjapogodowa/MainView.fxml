<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<ScrollPane prefHeight="596.0" prefWidth="1057.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <AnchorPane prefHeight="574.0" prefWidth="1053.0">
         <children>
            <Button fx:id="checkWeatherButton" layoutX="17.0" layoutY="150.0" mnemonicParsing="false" onAction="#CheckWeatherAction" text="check weather" />
            <Label layoutX="24.0" layoutY="47.0" text="citty" />
            <TextField fx:id="cityName" layoutX="84.0" layoutY="43.0" promptText="Your location" />
            <Label layoutX="22.0" layoutY="87.0" text="country" />
            <TextField fx:id="countryName" layoutX="84.0" layoutY="83.0" promptText="Your city" />
            <TextField fx:id="cityNameDestination" layoutX="340.0" layoutY="43.0" promptText="Your destination" />
            <TextField fx:id="countryNameDestination" layoutX="340.0" layoutY="83.0" promptText="Your destination country" />
            <TableView fx:id="TableOrigin" layoutX="12.0" layoutY="235.0" prefHeight="272.0" prefWidth="498.0">
              <columns>
                <TableColumn fx:id="dateOrigin" prefWidth="81.0" text="date" />
                <TableColumn fx:id="temperatureOrigin" prefWidth="51.0" text="temp" />
                  <TableColumn fx:id="pressureOrigin" prefWidth="78.0" text="pressure" />
                  <TableColumn fx:id="windspeedOrigin" prefWidth="90.0" text="wind speed" />
                  <TableColumn fx:id="rainOrigin" prefWidth="53.0" text="rain" />
                  <TableColumn fx:id="descriptionOrigin" prefWidth="144.0" text="description" />
              </columns>
            </TableView>
            <TableView fx:id="TableDestination" editable="true" layoutX="527.0" layoutY="235.0" prefHeight="271.0" prefWidth="504.0">
               <columns>
                  <TableColumn fx:id="dateDestination" prefWidth="81.0" text="date" />
                  <TableColumn fx:id="temperatureDestination" prefWidth="51.0" text="temp" />
                  <TableColumn fx:id="pressureDestination" prefWidth="78.0" text="pressure" />
                  <TableColumn fx:id="windSpeedDestination" prefWidth="90.0" text="wind speed" />
                  <TableColumn fx:id="rainDestination" prefWidth="53.0" text="rain" />
                  <TableColumn fx:id="descriptionDestination" prefWidth="150.0" text="description" />
               </columns>
            </TableView>
            <Label fx:id="originTableName" layoutX="22.0" layoutY="202.0" text="Weather inYour location" />
            <Label fx:id="destinationTableName" layoutX="527.0" layoutY="211.0" text="Weather in location of Your destination" />
            <Label fx:id="textFieldErrorLabel" layoutX="14.0" layoutY="119.0" prefHeight="17.0" prefWidth="358.0" text="Label" textFill="#e13434" />
         </children>
      </AnchorPane>
   </content>
</ScrollPane>
